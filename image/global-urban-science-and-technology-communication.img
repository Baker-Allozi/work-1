<script type="text/javascript">
    function closeHeadTwo() {
        $("#footer_new").css("margin-bottom","");
    }

    // 手机端个人中心点击
    $(function () {
        try{
            $('img').each(function () {
                if($(this).attr("alt") == undefined){
                    $(this).attr("alt",'');
                }
            });
        }catch (e) {}
        try {
            $(".lazy").lazyload({
                threshold: 50
            });
            if ( $(document).width() > 1100 ) {
                // load css start
                var h   = document.getElementsByTagName('head')[0];
                var s   = document.createElement('link');
                s.type  = 'text/css';
                s.rel  = 'stylesheet';
                s.href = "https://static.lovepik.com/css/lovepik/active/2022bd.v1.0.css";
                s.async = true;
                h.appendChild(s);
                // load css end
                function addBG(imgObj,dom,callback) {
                    if(imgObj.width > 0){
                        callback(dom,imgObj.src);
                    }else{
                        setTimeout(function () {
                            addBG(imgObj,dom,callback);
                        },10);
                    }
                }

                function loadcss(src, fn) {
                    var node = src;
                    if (node.attachEvent) {
                        //IE
                        node.attachEvent('onload', function () {
                            fn(null, node)
                        });
                    } else {
                        //other browser
                        setTimeout(function () {
                            poll(node, fn);
                        }, 0);
                    }
                    function poll(node, callback) {
                        var isLoaded = false;
                        if (/webkit/i.test(navigator.userAgent)) {
                            //webkit
                            if (node['sheet']) {
                                isLoaded = true;
                            }
                        } else if (node['sheet']) {
                            // for Firefox
                            try {
                                if (node['sheet'].cssRules) {
                                    isLoaded = true;
                                }
                            } catch (ex) {
                                // NS_ERROR_DOM_SECURITY_ERR
                                if (ex.code === 1000) {
                                    isLoaded = true;
                                }
                            }
                        }
                        if (isLoaded) {
                            setTimeout(function () {
                                callback(null, node);
                            }, 1);
                        } else {
                            setTimeout(function () {
                                poll(node, callback);
                            }, 10);
                        }
                    }
                    node.onLoad = function () {
                        fn(null, node);
                    }
                }
                //判断css加载完成与否
                loadcss(s,function (e) {
                    $(".anniversary_head_two").show();
                    if( $(".anniversary_head_two").length > 0
                        && $(".anniversary_head_two").css("display") == 'block'
                       ){
                        $("#footer_new").css("margin-bottom","40px");
                    }
                    //圣诞活动倒计时
                    env.show_time($(".sd-shi"),$(".sd-fen"),$(".sd-miao"),"1970/01/01 08:00",$(".sd-hm"));
                    var count = 0,that;
                    var length = $(".act_lazy").length;
                    for (var i = 0;i <= length;i++) {
                        that = $(".act_lazy").eq(count);count++;
                        if (!that.length) break;
                        var tan_url = that.attr('data-original');
                        if(tan_url && !that.attr('src')){
                            let IMG = new Image();
                            IMG.src = tan_url;
                            addBG(IMG,that,function (e,d) {
                                e.css("background-image","url(" + d +")");
                                if(d.match("childrenday_pop_topimg") ){
                                    if ( $(".super_act").length ) {
                                        var timer = setTimeout(function () {
                                            $(".super_act").fadeIn(30);
                                            $(".super_act").siblings().each(function () {$(this).css('filter','blur(5px)');});
                                            if( 8 == 8 ){bc(169);}else{bc(170);}
                                        },100);
                                    }
                                }
                            });
                        }
                        if(tan_url && that.attr('src')){
                            let IMG = new Image();
                            IMG.src = tan_url;
                            addBG(IMG,that,function (e,d) {
                                e.attr('src',d);
                            });
                        }
                    }
                });
            }
        } catch (err) {
            console.log(err)
        }

